<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Single">
    <test name="BStackDemo 1">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.browserstack.plain.LoginTest">
                <methods>
                    <include name="loginSuccess"/>
                    <include name="loginFail"/>
                    <include name="loginLockedUser"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="BStackDemo 2">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.browserstack.plain.LoginTest">
                <methods>
                    <include name="loginSuccess"/>
                    <include name="loginFail"/>
                    <include name="loginLockedUser"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>